{
  "name": "africa-investment",
  "version": "1.0.0",
  "description": "Application web de formulaire d'investissement pour l'Afrique",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "echo 'No build step needed for backend only'",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build",
    "test": "cd client && npm test"
  },
  "keywords": [
    "react",
    "nodejs",
    "express",
    "investment",
    "africa",
    "form",
    "api"
  ],
  "author": "Votre Nom",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.10"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
